        "@nuxtjs/pwa": "^2.6.0",
        "@nuxtjs/router": "^1.3.2",
        "@nuxtjs/vuetify": "^0.5.6",
        "axios": "^0.19.0",
        "cross-env": "^5.2.0",
        "dotenv": "^8.0.0",
        "jquery": "^3.4.1",
        "js-cookie": "^2.2.0",
        "nuxt": "^2.8.1",
        "nuxt-material-design-icons": "^1.0.4",
        "popper.js": "^1.15.0",
        "sweetalert2": "^8.13.0",
        "vee-validate": "^2.2.11",
        "vform": "^1.0.1",
        "vue-avatar": "^2.1.7",
        "vue-currency-input": "^1.6.0",
        "vue-i18n": "^8.11.2",
        "vue-recaptcha": "^1.2.0",
        "vue-recaptcha-v3": "^1.5.0",
        "vuetify": "^1.5.16",
        "vuetify-loader": "^1.2.2",
        "vuex": "^3.1.1",
        "vuex-persistedstate": "^2.5.4"
    },
    "devDependencies": {
        "@nuxtjs/eslint-config": "0.0.1",
        "@vue/cli-plugin-eslint": "^3.8.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.2",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-jest": "^22.7.1",
        "eslint-plugin-node": "^9.1.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-plugin-vue": "^5.2.3",
        "node-sass": "^4.12.0",
        "nodemon": "^1.19.1",
        "prettier": "^1.18.2",
        "sass-loader": "^7.1.0",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-console": "off"
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    }
}
